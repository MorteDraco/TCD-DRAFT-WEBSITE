                            function CheckLoginInDB($username,$password)
                        {
                            if(!$this->DBLogin())
                            {
                                $this->HandleError("Database login failed!");
                                return false;
                            }          
                            $username = $this->SanitizeForSQL($username);
                            $pwdmd5 = md5($password);
